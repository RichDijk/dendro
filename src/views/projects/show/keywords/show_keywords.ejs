<ul class="list-group" id="keyword_explorer_area">

    <a ng-repeat="keyword in keywords"
       ng-show="keywords != null"
       ng-click="clicked_term_explorer_node($index)"
       nc-blur="clear_selected_terms()"
       ng-class="{active: keyword.selected}"
       href="#"
       class="list-group-item ng-hide">
        <div class="row">
            <div class="col-lg-12">
                <h7 class="list-group-item-heading">
                    <table class="explorer-table">
                        <tr>
                            <td ng-show="multiple_term_selection">
                                <img src="/images/icons/check_box_uncheck.png" ng-show="!keyword.selected">
                                <img src="/images/icons/check_box.png" ng-show="keyword.selected">
                            </td>
                            <td class="term-tablecell">
                                <medium>{{keyword.words}}</medium>
                                <div class="help-block">
                                </div>
                            </td>
                        </tr>
                    </table>
                </h7>
            </div>
        </div>
    </a>
</ul>
