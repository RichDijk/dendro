<div ng-cloak>
    <div class="container">
        <h3>File Versions Timeline</h3>
        <hr>
        <ul class="list-group">
            <div ng-if="fileVersions.length==0" style="text-align: center">
                <h4>There are no file versions</h4>
                <i class="fa fa-file-o fa-5x" aria-hidden="true"></i>
            </div>
            <p>FileVersions Length : {{fileVersions.length}}</p>
            <div dir-paginate="fVersion in fileVersions | itemsPerPage: fileVersionsPerPage" total-items="totalFileVersions" current-page="pagination.current">
                <p> The URI IS : {{fVersion.fileVersion}}</p>
                <div ng-init="getFileVersion(fVersion.fileVersion);">
                    <div ng-if="fileVersionsList[fVersion.fileVersion].rdf.isShare">
                        <p> is share </p>
                        <ng-include src="'/app/views/social/showShareFileVersion.html'" ng-init="shareUri = fVersion.fileVersion; initSingleFileVersionShare(shareUri);"></ng-include>
                    </div>
                    <div ng-if="!fileVersionsList[fVersion.fileVersion].rdf.isShare">
                        <p> is fileVersion </p>
                        <ng-include src="'/app/views/social/showFileVersion.html'" ng-init="fileVersionUri = fVersion.fileVersion; initSingleFileVersion(fileVersionUri);"></ng-include>
                    </div>
                </div>
                <hr>
            </div>
        </ul>
    </div>

    <div>
        <div class="text-center">
            <dir-pagination-controls boundary-links="true" on-page-change="pageChangeHandlerFVersion(newPageNumber)" template-url="/bower_components/angular-utils-pagination/dirPagination.tpl.html"></dir-pagination-controls>
        </div>
    </div>
</div>