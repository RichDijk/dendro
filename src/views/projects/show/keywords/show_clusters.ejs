<ul class="list-group" id="cluster_explorer_area">

    <a ng-repeat="cluster in clusters"
       ng-show="cluster != null"
       href="#"
       class="list-group-item ng-hide">
        <div class="row">
            <div class="col-lg-12" ng-click="clickCluster(cluster)">
                <h7 class="list-group-item-heading">
                    <table class="explorer-table" >
                        <tr ng-repeat="term in cluster">
                            <td ng-click="clickTerm(term); $event.stopPropagation()">{{term.words}}</td>
                        </tr>
                    </table>
                </h7>
            </div>
        </div>
    </a>
</ul>